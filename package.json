{
  "name": "linter-flexpmd",
  "main": "./lib/init",
  "linter-package": true,
  "activationEvents": [],
  "version": "0.1.9",
  "private": true,
  "description": "Linting/analyzing ActionScript/Flash/Flex/AIR code on the fly, using flexpmd",
  "repository": "https://github.com/AtomLinter/linter-flexpmd.git",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "flexpmd": "1.3.0-2"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticCommits": true,
    "rangeStrategy": "pin",
    "packageRules": [
      {
        "packagePatterns": [
          "^eslint"
        ],
        "groupName": "ESLint packages"
      }
    ]
  }
}
